{
  "name": "cxs",
  "version": "4.0.0-0",
  "description": "Fast af css-in-js in 1kb",
  "main": "dist/index.js",
  "scripts": {
    "prepublish": "babel src -d dist",
    "build": "NODE_ENV=production webpack -p",
    "static": "static-react docs/App.js > docs/index.html",
    "start": "webpack-dev-server",
    "size": "npm run prepublish && bundlesize",
    "standard": "standard",
    "cover": "npm test && nyc report --reporter=html --reporter-lcov",
    "test": "nyc ava"
  },
  "keywords": [],
  "author": "Brent Jackson",
  "license": "MIT",
  "dependencies": {
    "objss": "^1.0.3",
    "tag-hoc": "^1.0.0-0"
  },
  "devDependencies": {
    "ava": "^0.21.0",
    "babel-cli": "^6.24.1",
    "babel-loader": "^7.1.1",
    "babel-plugin-module-resolver": "^2.7.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "browser-env": "^3.1.0",
    "bundlesize": "^0.12.1",
    "bytes": "^2.5.0",
    "loglevel": "^1.4.1",
    "nyc": "^11.0.3",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-live": "^1.7.0",
    "react-test-renderer": "^15.6.1",
    "standard": "^10.0.2",
    "static-react": "^4.0.0-2",
    "webpack": "^3.3.0",
    "webpack-dev-server": "^2.6.0"
  },
  "ava": {
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "nyc": {
    "exclude": [
      "test",
      "dist",
      "docs"
    ]
  },
  "standard": {
    "ignore": [
      "/docs/"
    ]
  },
  "bundlesize": [
    {
      "path": "./dist/index.js",
      "maxSize": "1.1 kb"
    },
    {
      "path": "./dist/component.js",
      "maxSize": "1.1 kb"
    },
    {
      "path": "./dist/atomic.js",
      "maxSize": "2 kb"
    }
  ],
  "static-react": {
    "title": "cxs",
    "meta": [
      {
        "name": "viewport",
        "content": "width=device-width,initial-scale=1"
      },
      {
        "name": "descriptoin",
        "content": "fast af css-in-js in 1kb"
      },
      {
        "name": "twitter:card",
        "content": "summary"
      },
      {
        "name": "twitter:site",
        "content": "summary"
      },
      {
        "name": "twitter:title",
        "content": "cxs"
      },
      {
        "name": "twitter:description",
        "content": "fast af css-in-js in 1kb"
      },
      {
        "name": "twitter:image",
        "content": "http://jxnblk.com/cxs/cxs.png"
      },
      {
        "name": "x",
        "content": "x"
      }
    ],
    "scripts": [
      "bundle.js",
      "meta.js"
    ]
  }
}
