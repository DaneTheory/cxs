{
  "name": "cxs",
  "version": "4.0.0-0",
  "description": "High performance, lightweight functional CSS-in-JS",
  "main": "dist/index.js",
  "scripts": {
    "prepublish": "babel src -d dist",
    "build": "webpack -p",
    "start": "webpack-dev-server",
    "size": "npm run prepublish && bundle-size .",
    "standard": "standard",
    "cover": "nyc report --reporter=html --reporter-lcov",
    "test": "nyc ava"
  },
  "keywords": [],
  "author": "Brent Jackson",
  "license": "MIT",
  "devDependencies": {
    "ava": "^0.21.0",
    "babel-cli": "^6.24.1",
    "babel-loader": "^7.1.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "browser-env": "^3.1.0",
    "bundle-size": "^1.1.5",
    "nyc": "^11.0.3",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-test-renderer": "^15.6.1",
    "standard": "^10.0.2",
    "webpack": "^3.3.0",
    "webpack-dev-server": "^2.6.0"
  },
  "dependencies": {},
  "ava": {
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "nyc": {
    "exclude": [
      "test",
      "dist",
      "docs"
    ]
  }
}
